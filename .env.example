# Backend Environment Variables Template
# Copy this to .env and add your actual keys

# AI Services
GROQ_API_KEY=your_groq_key_here
GROQ_MODEL=llama-3.1-8b-instant

# Location Services  
GEONAMES_USERNAME=your_geonames_username
GEONAMES_MAX_ROWS=100
GEOAPIFY_API_KEY=your_geoapify_key
LOCATIONIQ_KEY=your_locationiq_key
OPENTRIPMAP_KEY=your_opentripmap_key
OPEN_CAGE=your_open_cage_key

# Map Services
MAPILLARY_TOKEN=your_mapillary_token

# Content Services
SANITY_PROJECT_ID=your_sanity_project_id
SANITY_DATASET=production
SANITY_READ_TOKEN=your_sanity_read_token

# API Services
OLLAMA_API_KEY=your_ollama_api_key
CONTEXTSTREAM_API_KEY=your_contextstream_key
CONTEXTSTREAM_API_URL=https://api.contextstream.io

# Development Settings
USE_MOCK_DATA=false
